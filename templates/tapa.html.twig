{#
/**
 * @file tapa.html.twig
 * Default theme implementation to present Tapa data.
 *
 * This template is used when viewing Tapa pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_tapa()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('tapa') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
